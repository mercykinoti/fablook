<div class="container">
  <div id="myCarousel" class="carousel slider" data-ride="carousel">
    <!-- Indicators -->
    <ol class="carousel-indicators">
      <li data-target="#myCarousel" data-slide-to="0" class="active"></li>
      <li data-target="#myCarousel" data-slide-to="1"></li>
      <li data-target="#myCarousel" data-slide-to="2"></li>
    </ol>

    <!-- Wrapper for slides -->
    <div class="carousel-inner">

      <div class="item active">
        <%= image_tag(image_path('fab2.jpg')) %>
        <div class="carousel-caption">
        	<h3>
            <%= link_to 'Fablook', welcome_index_path %>
        	</h3>
          <p>A world of fashion</p>
        </div>
      </div>

      <div class="item">
      <%= image_tag(image_path('fab2.jpg')) %>
        <div class="carousel-caption">
        	<h3>
            <%= link_to 'Fablook', welcome_index_path %>
        	</h3>
          <p>Discover the best products that Carouselest suit your style</p>
        </div>
      </div>

      <div class="item">
        <%= image_tag(image_path('fab2.jpg')) %>
        <div class="carousel-caption">
        	<h3>
        		<a href="sale.html">Looking for a sale?</a>
        	</h3>
          <p> Get notifications when the products you liked go on sale</p>
        </div>
      </div>

    </div>


  </div>
</div>




